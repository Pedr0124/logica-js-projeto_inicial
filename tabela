

let atividade = [
    [acordar, 6, 30],
    [curso, 7, 30],
    [almoço, 12, 30]
]

function TabelaAleatória(tabela){
for(i=0; i<tabela.length; i++){
    for(j=0; j<tabela[i].length; j++){
        tabela[i][j]= gerarnumeroaleatório(100);
    }
}
return tabela;
}
function gerarnumeroaleatório(index){
    return Math.floor(Math.random()*index+1);
}